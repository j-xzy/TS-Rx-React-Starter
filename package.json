{
  "name": "ts-rx-react-starter",
  "version": "1.0.0",
  "scripts": {
    "start": "node ./build/devServer.js",
    "build": "webpack-cli --config ./build/webpack/webpack.prod.js",
    "analyze": "webpack-cli --config ./build/webpack/webpack.analyze.js",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx --fix",
    "test": "npm run type-check && jest __tests__ --config=jest.config.js",
    "coverage": "jest test --config=jest.config.js --coverage",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "mock": "node ./build/script/doMock.js"
  },
  "sideEffects": [
    "*.css",
    "*.styl",
    "*.scss",
    "*/store/**/*.ts",
    "*/config/**/*.ts",
    "*/lib/ajax.ts"
  ],
  "dependencies": {
    "immutability-helper": "^3.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "redux-logger": "^3.0.6",
    "rxjs": "^6.6.0",
    "type-redux": "^2.0.2",
    "type-redux-hook": "^2.0.0",
    "type-redux-rxjs": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-loader": "^8.1.0",
    "chokidar": "^3.4.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "open": "^7.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.2.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^3.0.6",
    "ts-faker": "0.0.5",
    "typescript": "^3.9.6",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-merge": "^5.0.9"
  }
}
